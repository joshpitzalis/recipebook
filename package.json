{
  "name": "recipebook",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.14.1",
    "firebase": "^5.9.1",
    "grommet": "^2.5.5",
    "grommet-icons": "^4.2.0",
    "immer": "^2.1.4",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-automata": "^4.0.4",
    "react-beautiful-dnd": "^10.1.0",
    "react-dom": "^16.8.4",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.5",
    "react-test-renderer": "^16.8.4",
    "styled-components": "^4.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "jest": "react-scripts test",
    "eject": "react-scripts eject",
    "build:testConfig": "cypress-firebase createTestEnvFile",
    "cypress": "npm run build:testConfig && cross-env CYPRESS_baseUrl=http://localhost:3000 cypress open"
